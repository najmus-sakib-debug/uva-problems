#include<stdio.h>
int main()
{
    double a,b,c,d,e,f,g,h,i,j,k,l,m,n;
    int o,p,q,r,s,t,u,v,w,x,y,z,lastt;
    scanf("%lf", &n);
    o = n * 100;
    p = o /10000;
    a = o - (p * 10000);
    q = a / 5000;
    b = a - (q * 5000);
    r = b / 2000;
    c = b - (r * 2000);
    s = c / 1000;
    d = c - (s * 1000);
    t = d / 500;
    e = d - (t * 500);
    u = e / 200;
    f = e - (u * 200);
    v = f / 100;
    g = f - (v * 100);
    w = g / 50;
    h = g - (w * 50);
    x = h / 25;
    i = h - (x * 25);
